{
  "name": "craco-babel-loader",
  "version": "1.0.0",
  "description": "Rewire `babel-loader` loader in your `create-react-app` project using `craco`.",
  "source": "src/index.ts",
  "module": "dist/index.es.js",
  "main": "dist/index.js",
  "umd:main": "dist/index.umd.js",
  "repository": "https://github.com/rjerue/craco-babel-loader",
  "author": {
    "name": "Ryan Jerue",
    "email": "ryan@jerue.org",
    "url": "http://jerue.org"
  },
  "license": "MIT",
  "eslintConfig": {
    "parser": "babel-eslint",
    "env": {
      "browser": true,
      "node": true,
      "mocha": true
    },
    "extends": [
      "eslint:recommended"
    ]
  },
  "scripts": {
    "build": "microbundle",
    "dev": "microbundle watch --compress=false",
    "prepublish": "npm run build"
  },
  "keywords": [
    "babel-loader",
    "craco",
    "webpack",
    "create-react-app"
  ],
  "dependencies": {},
  "peerDependencies": {
    "@craco/craco": "^6.4.2"
